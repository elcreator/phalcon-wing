{
  "name": "phalcon-wing",
  "version": "0.1.0",
  "description": "Phalcon-based framework",
  "scripts": {
    "start": "shell-exec \"npm run watch-scss\" \"npm run watch-js\"",
    "watch-scss": "node-sass -wr --output-style compressed scss/index.scss -o www/css/styles.css",
    "watch-js": "watchify js/main.js -o www/js/main.js -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elcreator/phalcon-wing.git"
  },
  "author": "Artur Kyryliuk",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/elcreator/phalcon-wing/issues"
  },
  "homepage": "https://github.com/elcreator/phalcon-wing#readme",
  "devDependencies": {
    "node-sass": "^3.10.0",
    "shell-executor": "^0.3.2",
    "watchify": "^3.7.0"
  }
}
